{"head":{"title":"Commander.js","category":"JavaScript libraries"},"body":[{"title":"Initialize","elms":"\n    var cli = require('commander');\n","h2":null},{"title":"Options","elms":"\n    cli\n      .version(require('../package').version)\n      .usage('[options] <command>')\n      .option('-w, --words <n>', 'generate <n> words')\n      .option('-i, --interval <n>', 'interval [1000]', 1000)\n      .option('-s, --symbols', 'include symbols')\n      .parse(process.argv);\n","h2":null},{"title":"Help","elms":"\n    .on('--help', function() {\n      console.log('');\n    })\n","h2":null},{"title":"Commands","elms":"\n    cli.outputHelp();\n    cli.args == [\"hello\"];\n","h2":null},{"title":"Other useful things","elms":"\n    process.exit(0);\n\n","h2":null}]}